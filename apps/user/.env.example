# === Easiest Local Setup (no Keycloak/DB) ===
# Enables mock auth endpoints so you can register/login without Keycloak or DB
MOCK_AUTH=true

# Allow the web app origins during local dev
CORS_ORIGINS_DEV=http://localhost:3000,http://127.0.0.1:3000

# === Real Keycloak/DB Setup ===
PRIVATE_KEYCLOAK_REALM = treetracker

PRIVATE_KEYCLOAK_BASE_URL = https://dev-k8s.treetracker.org/keycloak

PRIVATE_KEYCLOAK_CLIENT_SECRET =

DATABASE_URL=

PRIVATE_KEYCLOAK_CLIENT_ID = wallet-app-user-dev-svc

PRIVATE_WALLET_API_KEY = FORTESTFORTESTFORTESTFORTESTFORTEST

CORS_ORIGINS=
CORS_ORIGINS_DEV=http://localhost:3000,http://localhost:3001,http://127.0.0.1:3000
