{
  "name": "user",
  "version": "1.0.0",
  "description": "greenstand-wallet-app-api",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prettier": "prettier --config ../../packages/config/prettier.config.json --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=dev nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "cross-env NODE_ENV=debug nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:win": "cross-env NODE_ENV=dev nest start",
    "lint": "eslint . --ignore-pattern '**/.next/**' --ignore-pattern '.history/**'",
    "lint-fix": "eslint . --ignore-pattern '**/.next/**' --ignore-pattern '.history/**' --fix",
    "pre-commit": "lint-staged",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:unit": "jest --config ./unit-tests.config.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e-tests.config.ts",
    "int-test": "jest --config ./int-tests.config.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^3.1.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.4.2",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@treetracker/config": "0.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "28.1.3",
    "lint-staged": "^15.2.11",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  }
}
