{
  "name": "treetracker-wallet-app",
  "version": "1.0.0",
  "packageManager": "yarn@4.9.4",
  "main": "index.js",
  "repository": "",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "web:dev": "yarn workspace web dev",
    "web:prod": "yarn workspace web start",
    "web:build": "yarn workspace build",
    "native:start": "yarn workspace native start",
    "native:android": "yarn workspace native android",
    "native:ios": "yarn workspace native ios",
    "user:build": "yarn workspace user build",
    "user:dev": "yarn workspace user start:dev",
    "user:debug": "yarn workspace user start:debug",
    "user:prod": "yarn workspace user start:prod",
    "user:test:unit": "yarn workspace user test:unit",
    "user:test:unit:watch": "yarn workspace user test:watch",
    "user:test:int": "yarn workspace user int-test",
    "user:test:cov": "yarn workspace user test:cov",
    "user:test:debug": "yarn workspace user test:debug",
    "user:test:e2e": "yarn workspace user test:e2e",
    "pre-commit": "lint-staged",
    "cypress-component-test": "yarn workspace web run cy:component",
    "cypress-e2e-test": "yarn workspace web cy:e2e",
    "cypress-e2e-headless-test": "yarn workspace web cy:e2e-headless",
    "commitlint": "commitlint",
    "lint-staged": "lint-staged",
    "lint:check": "eslint . --no-warn-ignored",
    "lint:fix": "eslint . --fix --no-warn-ignored",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "husky",
    "prepare": "husky",
    "bdd:e2e": "yarn workspace @treetracker/bdd test",
    "bdd:e2e:local": "yarn workspace @treetracker/bdd test:local",
    "bdd:e2e:login": "yarn workspace @treetracker/bdd test:login",
    "bdd:e2e:register": "yarn workspace @treetracker/bdd test:register",
    "bdd:e2e:debug": "yarn workspace @treetracker/bdd test:debug",
    "bdd:e2e:update-driver": "yarn workspace @treetracker/bdd test:update-driver",
    "bdd:report": "yarn workspace @treetracker/bdd report:cucumber",
    "bdd:test:report": "yarn workspace @treetracker/bdd test:report",
    "bdd:test:local:report": "yarn workspace @treetracker/bdd test:local:report",
    "native:bdd:test": "yarn workspace native wdio",
    "build-apk-android": "yarn workspace native run build-android",
    "prebuild": "yarn workspace native prebuild"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "nohoist": [
      "**/react",
      "**/react-dom",
      "!apps/web/react",
      "!apps/web/react-dom",
      "**/expo-constants"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@wdio/devtools-service": "^8.42.0",
    "conventional-changelog-conventionalcommits": "^9",
    "eslint": "^9.38.0",
    "eslint-config-expo": "^10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.11",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@treetracker/config": "0.0.1"
  }
}
